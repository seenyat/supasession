{
  "name": "supasession",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r run dev",
    "dev:web": "pnpm --filter @supasession/web dev",
    "dev:relay": "pnpm --filter @supasession/relay dev",
    "build": "pnpm -r run build",
    "build:web": "pnpm --filter @supasession/web build",
    "build:relay": "pnpm --filter @supasession/relay build",
    "build:extension": "pnpm --filter @supasession/extension build",
    "check": "pnpm -r run check"
  },
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
